{
	"name": "dtfmovies",
	"properties": {
		"folder": {
			"name": "FormacaoED"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dt_transf_movie",
						"type": "DatasetReference"
					},
					"name": "MoviesDL"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "FilmesAcima1950"
				},
				{
					"name": "ColunaDerivada"
				},
				{
					"name": "SelectCorrigeNomeColuna"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movie as string,",
				"          title as string,",
				"          genres as string,",
				"          year as string,",
				"          Rating as string,",
				"          {Rotton Tomato} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesDL",
				"SelectCorrigeNomeColuna filter(/************************************** \r",
				"Converter o ano de string para inteiro e selecionar apenas os maiores que 1950\r",
				"**************************************/\r",
				"toInteger(year) > 1950) ~> FilmesAcima1950",
				"FilmesAcima1950 derive(PrimeiroGenero = iif(locate('|', genres) > 1, left(genres, locate(('|'), genres) -1), genres)) ~> ColunaDerivada",
				"MoviesDL select(mapColumn(",
				"          movie,",
				"          title,",
				"          genres,",
				"          year,",
				"          Rating,",
				"          {Rotten Tomato} = {Rotton Tomato}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectCorrigeNomeColuna"
			]
		}
	}
}