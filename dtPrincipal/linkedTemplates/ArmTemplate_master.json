{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dtPrincipal"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"AzureDataLakeStorageArquivoFinal_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageArquivoFinal'"
		},
		"RestServiceOrganizze_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestServiceOrganizze'"
		},
		"SqlServerMovies_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerMovies'"
		},
		"SqlServerOrganizze_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerOrganizze'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saformacaoedaj.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageArquivoFinal_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saformacaoedaj.dfs.core.windows.net/"
		},
		"HttpServerMovie_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/djpmsft/adf-ready-demo/master/moviesDB.csv"
		},
		"RestServiceOrganizze_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.organizze.com.br/rest/v2/"
		},
		"RestServiceOrganizze_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "juniorgodoy@outlook.com.br"
		},
		"RestServiceYan_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://involt.bitrix24.com.br/bitrix/tools/biconnector/pbi.php?token=8BEAhnwieeyCr4BXlzSXWkX5x1TADigzbr&table=crm_deal"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dtPrincipal_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"AzureDataLakeStorageArquivoFinal_accountKey": {
						"value": "[parameters('AzureDataLakeStorageArquivoFinal_accountKey')]"
					},
					"RestServiceOrganizze_password": {
						"value": "[parameters('RestServiceOrganizze_password')]"
					},
					"SqlServerMovies_connectionString": {
						"value": "[parameters('SqlServerMovies_connectionString')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageArquivoFinal_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageArquivoFinal_properties_typeProperties_url')]"
					},
					"HttpServerMovie_properties_typeProperties_url": {
						"value": "[parameters('HttpServerMovie_properties_typeProperties_url')]"
					},
					"RestServiceOrganizze_properties_typeProperties_url": {
						"value": "[parameters('RestServiceOrganizze_properties_typeProperties_url')]"
					},
					"RestServiceOrganizze_properties_typeProperties_userName": {
						"value": "[parameters('RestServiceOrganizze_properties_typeProperties_userName')]"
					},
					"RestServiceYan_properties_typeProperties_url": {
						"value": "[parameters('RestServiceYan_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dtPrincipal_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SqlServerOrganizze_connectionString": {
						"value": "[parameters('SqlServerOrganizze_connectionString')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dtPrincipal_ArmTemplate_0')]"
			]
		}
	]
}