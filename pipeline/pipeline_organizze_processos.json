{
	"name": "pipeline_organizze_processos",
	"properties": {
		"activities": [
			{
				"name": "CopyData",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"additionalColumns": [
							{
								"name": "DATA LOADING",
								"value": {
									"value": "@formatDateTime(utcNow(),'yyyy-MM-ddTHH:mm:ssZ')",
									"type": "Expression"
								}
							}
						],
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": false,
							"keys": [
								"ID"
							],
							"interimSchemaName": ""
						},
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "DS_Organizze",
						"type": "DatasetReference",
						"parameters": {
							"url_relativa": {
								"value": "@pipeline().parameters.url_relativa",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_SqlServerOrganizzeCategoria",
						"type": "DatasetReference",
						"parameters": {
							"schema": {
								"value": "@pipeline().parameters.schema",
								"type": "Expression"
							},
							"tabela": {
								"value": "@pipeline().parameters.tabela",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"url_relativa": {
				"type": "string"
			},
			"schema": {
				"type": "string",
				"defaultValue": "dbo"
			},
			"tabela": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Organizze"
		},
		"annotations": []
	}
}